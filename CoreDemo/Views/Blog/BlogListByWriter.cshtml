@using EntityLayer.Concrete
@model List<Blog>
@{
    Layout = "WriterLayout";
}
<p1>deneme</p1>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Blog Başlığı</th>
        <th>Oluşturma Tarihi</th>
        <th>Kategori</th>
        <th>Durum</th>
        <th>Sil</th>
        <th>Duzenle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.BlogID</td>
            <td>@item.BlogTitle</td>
            <td>@item.BlogCreateDate</td>
            <td>@item.CategoryID</td>
            <td>@item.BlogStatus</td>
            <td><a href="#" class="btn btn-danger">Sil</a></td>
            <td><a href="#" class="btn btn-warning">Düzenle</a></td>
        </tr>
    }
</table>
<a asp-controller="Blog" asp-action="BlogAdd" class="btn btn-primary">Yeni Blog Oluştur</a>